<div class="section-video">
  <video autoplay loop muted playsinline width="100%" {% if section.settings.height != 'use_image' %} style="top: 0;  left: 0;  width: 100%;  height: auto"{% endif %}>
    <source src="{{ section.settings.videofile }}" type="video/mp4" id="{{ section.id }}">
  </video> 
  <div class="video-cover-image">
    <h1>{{ section.settings.headingcus }}</h1>
  </div>
</div>
  

{% schema %}
  {
    "name": "custom video",
    "settings": [
      {
        "type": "text",
        "id": "videofile",
        "label": "Video URL",
        "default": "https://cdn.shopify.com/s/files/1/2018/8867/files/ice.mp4"
      },
      
    ],
     "presets": [
      {
        "name": "banner custom video"
      }
    ]
  }
{% endschema %}

